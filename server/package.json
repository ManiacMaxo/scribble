{
  "name": "scribble-client",
  "author": "Victor Gorchilov",
  "private": true,
  "version": "0.0.1",
  "main": "build/index.js",
  "scripts": {
    "build": "swc src -d build",
    "build:tiny": "NODE_ENV=production spack",
    "dev": "nodemon .",
    "start": "NODE_ENV=production node .",
    "start:dev": "NODE_ENV=development node ."
  },
  "nodemonConfig": {
    "watch": [
      "src/",
      ".env"
    ],
    "ext": "ts",
    "exec": "yarn build && yarn start:dev"
  },
  "dependencies": {
    "@fastify/cors": "^8.1.0",
    "@fastify/type-provider-json-schema-to-ts": "^2.1.1",
    "@prisma/client": "^4.4.0",
    "axios": "^0.27.2",
    "dotenv": "^16.0.3",
    "fastify": "^4.6.0",
    "fastify-socket.io": "^4.0.0",
    "lodash": "^4.17.21",
    "socket.io": "^4.5.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.4",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.7.23",
    "@types/uuid": "^8.3.4",
    "chokidar": "^3.5.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "prisma": "^4.4.0",
    "typescript": "^4.8.4"
  }
}